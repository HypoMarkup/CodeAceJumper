{
  "name": "codeacejumper",
  "displayName": "Code Ace Jumper",
  "description": "Visual Studio Code Ace Jump",
  "version": "1.0.0",
  "publisher": "LucaTrazzi",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.aceJump"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.aceJump",
        "title": "Ace Jump"
      }
    ],
    "keybindings": [
      {
        "command": "extension.aceJump.input.stop",
        "key": "escape",
        "when": "editorTextFocus && aceJumpInput"
      }
    ],
    "configuration": {
      "title": "Ace Jump",
      "type": "object",
      "properties": {
        "aceJump.placeholder.backgroundColor": {
          "type": "string",
          "default": "yellow"
        },
        "aceJump.placeholder.color": {
          "type": "string",
          "default": "black"
        },
        "aceJump.placeholder.border": {
          "type": "string",
          "default": "dotted thin black"
        },
        "aceJump.finder.pattern": {
          "type": "string",
          "default": "[ ,-.{_(\\[]"
        }        
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/lodash": "^4.14.37",
    "lodash": "^4.16.4"
  }
}