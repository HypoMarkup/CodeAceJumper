{
  "name": "codeacejumper",
  "displayName": "Code Ace Jumper",
  "description": "Visual Studio Code Ace Jump",
  "version": "1.0.0",
  "publisher": "LucaTrazzi",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.aceJump"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.aceJump",
        "title": "Ace Jump"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "@types/lodash": "^4.14.37",
    "lodash": "^4.16.4"
  }
}
