{
  "name": "codeacejumper",
  "displayName": "Code Ace Jumper",
  "description": "Visual Studio Code Ace Jump",
  "icon": "images/ace-jumper.png",
  "version": "1.1.10",
  "publisher": "lucax88x",
  "homepage": "https://github.com/lucax88x/CodeAceJumper",
  "license": "https://github.com/lucax88x/CodeAceJumper/license.md",
  "keywords": [
    "jump",
    "AceJump",
    "EasyMotion"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lucax88x/CodeAceJumper.git"
  },
  "bugs": {
    "url": "https://github.com/lucax88x/CodeAceJumper/issues",
    "email": "lucax88x@gmail.com"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.aceJump",
        "title": "AceJump: Jump"
      },
      {
        "command": "extension.aceJump.multiChar",
        "title": "AceJump: MultiChar"
      },
      {
        "command": "extension.aceJump.selection",
        "title": "AceJump: Selection"
      }
    ],
    "keybindings": [
      {
        "command": "extension.aceJump.input.stop",
        "key": "escape",
        "when": "editorTextFocus && aceJumpInput"
      }
    ],
    "configuration": {
      "title": "Ace Jump",
      "type": "object",
      "properties": {
        "aceJump.placeholder.backgroundColor": {
          "type": "string",
          "default": "yellow"
        },
        "aceJump.placeholder.color": {
          "type": "string",
          "default": "black"
        },
        "aceJump.placeholder.border": {
          "type": "string",
          "default": "dotted thin black"
        },
        "aceJump.placeholder.width": {
          "type": "number",
          "default": 12
        },
        "aceJump.placeholder.height": {
          "type": "number",
          "default": 14
        },
        "aceJump.placeholder.fontSize": {
          "type": "number",
          "default": 14
        },
        "aceJump.placeholder.textPosX": {
          "type": "number",
          "default": 2
        },
        "aceJump.placeholder.textPosY": {
          "type": "number",
          "default": 12
        },
        "aceJump.placeholder.fontWeight": {
          "type": "string",
          "default": "normal"
        },
        "aceJump.placeholder.fontFamily": {
          "type": "string",
          "default": "Consolas"
        },
        "aceJump.placeholder.upperCase": {
          "type": "boolean",
          "default": false
        },
        "aceJump.highlight.backgroundColor": {
          "type": "string",
          "default": "rgba(124,240,10,0.5)"
        },
        "aceJump.highlight.width": {
          "type": "number",
          "default": 9
        },
        "aceJump.highlight.height": {
          "type": "number",
          "default": 14
        },
        "aceJump.highlight.offsetX": {
          "type": "number",
          "default": 0
        },
        "aceJump.highlight.offsetY": {
          "type": "number",
          "default": 0
        },
        "aceJump.finder.pattern": {
          "type": "string",
          "default": "[ ,-.{_(\"'<\\[]"
        },
        "aceJump.finder.skipSelection": {
          "type": "boolean",
          "default": false
        },
        "aceJump.finder.onlyInitialLetter": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run lint && yarn run compile",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/ramda": "^0.25.50",
    "@types/sinon": "^7.0.6",
    "@types/xmlbuilder": "0.0.28",
    "mocha": "^5.2.0",
    "sinon": "^7.2.4",
    "tslint": "^5.8.0",
    "typescript": "^3.1.3",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "ramda": "^0.26.1",
    "xmlbuilder": "^8.2.2"
  }
}
